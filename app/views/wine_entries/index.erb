<h2>ALL WINE ENTRIES</h2>
<br>
<br>
<% @wine_entries.each do |wine_entry| %>
    <a href="/wine_entries/<%= wine_entry.id %>"><%= wine_entry.formatted_created_at %></a>
    <p><strong>Name of Wine:</strong> <%= wine_entry.wine_name %></p>
    <p><strong>Type:</strong> <%= wine_entry.wine_type %></p>
    <p><strong>Vintage:</strong> <%= wine_entry.vintage %></p>
    <p><strong>Region:</strong> <%= wine_entry.region %></p>
    <p><strong>Personal Notes:</strong> <%= wine_entry.wine_notes %></p>
    <p>By <%= wine_entry.user.name %></p>
    <br>
    <br>
<% end %>